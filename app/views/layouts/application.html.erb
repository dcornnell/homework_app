<!DOCTYPE html>
<html>
<head>
  <title>HomeworkApp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<% if current_user %>
	<%= link_to "Log_out", logout_path %>
			<% if current_user.admin == "teacher" %>
			teacher
	
			<% else %>
				student

	<br>
	<% end %>
	
<% else %>
	<%= link_to "Log_in", login_path %>

	<br>
	<% end %>

	

<%= yield %>
<br>

</body>
</html>
